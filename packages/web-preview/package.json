  "name": "web-preview",
  "version": "1.12.0",
  "private": true,
  "description": "Sirius Web",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "start:test": "yarn workspace env_def build test web && yarn workspace api build && node ./makeInstall && yarn dev",
    "start:test1": "yarn workspace env_def build test1 web && yarn workspace api build && node ./makeInstall && yarn dev",
    "start:test2": "yarn workspace env_def build test2 web && yarn workspace api build && node ./makeInstall && yarn dev:prod",
    "start": "yarn workspace env_def build local web && yarn workspace api build && node ./makeInstall && yarn dev",
    "dev": "gatsby develop -o -S -p 8000 -H su-desktop-web.cowork.netease.com --verbose",
    "buildtest1": "yarn workspace env_def build test1 win && yarn workspace api build && yarn build && yarn moveFileDev",
    "buildprev": "yarn workspace env_def build prev win && yarn workspace api build && yarn build && yarn moveFilePrev",
    "buildtestprod": "yarn workspace env_def build test_prod win && yarn workspace api build && yarn build && yarn moveFileTestProd",
    "buildprod": "yarn workspace env_def build prod win && yarn workspace api build && yarn build && yarn moveFile",
    "build": "gatsby clean && echo  'start build imgPreviewPage' && cross-env NODE_OPTIONS=--max-old-space-size=8192 gatsby build --verbose --prefix-paths",
    "moveFile": "rm -rf statics && mkdir statics && mkdir statics/imgPreview && cp -rf public/* statics/imgPreview",
    "moveFileDev": "rm -rf staticsdev && mkdir staticsdev && mkdir staticsdev/imgPreview && cp -rf public/* staticsdev/imgPreview",
    "moveFilePrev": "rm -rf staticsprev && mkdir staticsprev && mkdir staticsprev/imgPreview && cp -rf public/* staticsprev/imgPreview",
    "moveFileTestProd": "rm -rf staticsTestprod && mkdir staticsTestprod && mkdir staticsTestprod/imgPreview && cp -rf public/* staticsTestprod/imgPreview",
    "clean": "gatsby clean"
  },
