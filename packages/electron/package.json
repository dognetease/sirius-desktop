{
  "name": "sirius-desktop",
  "version": "1.34.10",
  "edmVersion": "1.232.1",
  "description": "网易灵犀办公客户端",
  "author": {
    "name": "网易灵犀"
  },
  "main": "dist/electron/index.js",
  "scripts": {
    "build": " yarn electron:build",
    "electron:build": "yarn compile && node ./electron_build && yarn afterAllBuildHook && yarn electron:buildMorePackage",
    "electron:buildOnly": "node ./electron_build",
    "electron:buildMorePackage": "node ./build-utils/build-more-package.js",
    "electron:buildWinMorePackage": "node ./build-utils/build-more-package.js --os=win",
    "electron:buildWin32MorePackage": "node ./build-utils/build-more-package.js --os=win32",
    "electron:buildMacMorePackage": "node ./build-utils/build-more-package.js --os=mac",
    "electron:buildMacM1Only": "node ./electron_build --os=macArm && yarn afterAllBuildHook",
    "electron:buildWin": "node ./electron_build --os=win && yarn afterAllBuildHook && yarn electron:buildWinMorePackage",
    "electron:buildMac": "node ./electron_build --os=mac && yarn afterAllBuildHook && yarn electron:buildMacMorePackage",
    "electron:buildWin32": "node ./electron_build --os=win32 && yarn afterAllBuildHook && yarn electron:buildWin32MorePackage",
    "electron:buildWinX64UpdateOnly": "node ./electron_build --config=electron-builder-win-update.yml --os=win && yarn afterAllBuildHook",
    "electron:buildWinWin32UpdateOnly": "node ./electron_build --config=electron-builder-win-update.yml --os=win32 && yarn afterAllBuildHook",
    "electron:buildWinUpdateOnly": "node ./electron_build --config=electron-builder-win-update.yml && yarn afterAllBuildHook",
    "electron:buildEdmWinUpdateOnly": "node ./electron_build --config=electron-builder-edm-win-update.yml && yarn afterAllBuildHook",
    "electron:buildEdmWinX64UpdateOnly": "node ./electron_build --config=electron-builder-edm-win-update.yml --os=win && yarn afterAllBuildHook",
    "electron:buildEdmWin32UpdateOnly": "node ./electron_build --config=electron-builder-edm-win-update.yml --os=win32 && yarn afterAllBuildHook",
    "electron:buildMsiOnly": "node ./electron_build --config=electron-builder-msi.yml",
    "electron:buildMsiOnlyTest": "node ./electron_build --config=electron-builder-msi-test.yml",
    "buildNewNginxConfig": "node ./build-utils/nginx-generate.js",
    "afterAllBuildHook": "node ./build-utils/after-all-build.js",
    "compile:start:dev": "yarn dev",
    "dev:prod": "yarn workspace api build:bk_init && yarn compile && yarn start",
    "dev": "yarn workspace api build:bk_init && yarn compile && yarn start",
    "dev:win": "yarn workspace env_def build:win:dev && yarn workspace web build && yarn compile && yarn start:dev",
    "test": "yarn workspace env_def build && yarn workspace web build && yarn compile && yarn start ",
    "test:win": "yarn workspace env_def build:win:dev && yarn workspace web build && yarn compile && yarn start ",
    "dev:lib": "cross-env compileType=lib rollup -c && yarn start:dev",
    "dev:main": "cross-env compileType=main rollup -c && yarn start:dev",
    "bundle": "rollup -c rollup.d.config.js",
    "library": "cross-env compileType=lib rollup -c ",
    "compile": "rollup -c",
    "start": "yarn workspace v8-snapshot copySnapshotToDevElectron && cross-env NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 electron dist/electron/index.js",
    "start:dev": "yarn workspace v8-snapshot copySnapshotToDevElectron && cross-env NODE_ENV=web NODE_TLS_REJECT_UNAUTHORIZED=0 electron dist/electron/index.js",
    "clean": "echo electron-clean",
    "delete:temp": "rm -rf ./build-utils/.temp",
    "delete:winTemp": "rm -r -fo ./build-utils/.temp"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/auto-launch": "^5.0.2",
    "cross-env": "^7.0.3",
    "electron": "22.3.27",
    "electron-builder": "^23.0.3",
    "electron-notarize": "^1.0.0",
    "rollup": "^2.44.0",
    "rollup-plugin-baked-env": "^1.0.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^2.1.0"
  },
  "dependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mailparser": "^3.4.0",
    "adm-zip": "^0.5.5",
    "auto-launch": "^5.0.5",
    "electron-updater": "^5.0.1",
    "fs-extra": "^10.0.0",
    "iconv-lite": "^0.6.3",
    "js-yaml": "^4.1.0",
    "lodash.debounce": "^4.0.8",
    "mailparser": "^3.5.0",
    "node-tnef": "^1.4.0",
    "original-fs": "^1.1.0"
  }
}
