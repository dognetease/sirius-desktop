  "scripts": {
    "build:edm_env": "yarn workspace env_def build edm_test web && yarn workspace api build && node ./makeInstall",
    "build:edm_fast_env": "$npm_package_config_edm_fast && yarn workspace env_def build edm_test web && yarn workspace api build && node ./makeInstall",
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=12288 gatsby develop -o -S -p 8000 -H su-desktop-web.cowork.netease.com --verbose",
    "dev:edm_fast": "cross-env NODE_OPTIONS=--max-old-space-size=12288 FASE_DEV_WEB=true EDM_FAST=true gatsby develop -o -S -p 8000 -H su-desktop-web.cowork.netease.com --verbose",
    "start:ffmsedm": "yarn workspace env_def build ffmsedm_test web && yarn workspace api build && node ./makeInstall && cross-env NODE_OPTIONS=--max-old-space-size=8192 yarn dev",
    "start:prev": "yarn workspace env_def build prod web && yarn workspace api build && node ./makeInstall && yarn dev",
    "lint": "yarn eslint src/**/*.tsx",
    "dev:prod": "gatsby develop -o -S -p 8100 -H su-desktop-web.office.163.com --verbose",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 gatsby build --verbose --prefix-paths && yarn workspace api build:bk_init && node ./makeInstall",
    "build:app": "yarn workspace env_def build test mac && yarn build",
    "build:web": "yarn workspace env_def build test web && yarn workspace api build && yarn build",
    "build:webmail": "yarn workspace env_def build webmail_test web && yarn workspace api build && yarn build",
    "build:webmail_prod": "yarn workspace env_def build webmail_prod web && yarn workspace api build && yarn build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
